<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cl.ciudadanointeligente.sil.model">
	<class name="Bill" table="ProyectoLey">
		<id name="id" column="id_proyecto_ley">
			<generator class="identity"/>
		</id>
		<property name="bulletinNumber" column="nro_boletin"/>
		<property name="title" column="titulo"/>
		<property name="sessionTitle" column="titulo_sesion"/>
		<property name="entryDate" column="fecha_ingreso"/>
		<property name="initiative" column="iniciativa"/>
		<property name="type" column="tipo"/>
		<property name="originChamber" column="camara_origen"/>
		<property name="urgency" column="urgencia"/>
		<property name="stage" column="etapa"/>
		<property name="substage" column="sub_etapa"/>
		<property name="law" column="ley"/>
		<property name="lawUrl" column="ley_bcn"/>
		<property name="decree" column="decreto"/>
		<property name="decreeUrl" column="decreto_bcn"/>
		<property name="publishDate" column="fecha_publicacion"/>
		<property name="matterId" column="id_materia"/>
		<property name="internalNumber" column="nro_interno"/>
		<property name="advance" column="avance"/>
		<property name="processingNumber" column="nro_tramitacion"/>
		<property name="processingActive" column="tramitacion_act"/>
		<property name="summary" column="resumen"/>
		<property name="createdAt" column="created_at"/>
		<property name="updatedAt" column="updated_at"/>
		<set name="authors" table="AutorProyectoLey">
			<key column="id_proyecto_ley"/>
			<many-to-many column="id_autor" class="Author"/>
		</set>
	</class>
</hibernate-mapping>
